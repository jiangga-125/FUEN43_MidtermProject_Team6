@model IEnumerable<ReportMail.Models.ReportExportLog>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReportName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExportFormat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExportAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Filters)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FilePath)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReportName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExportFormat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExportAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Filters)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FilePath)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ExportId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ExportId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ExportId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
